<settings>
  <profiles>
    <profile>
      <id>ossrh</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <gpg.executable>gpg</gpg.executable>
        <gpg.passphrase>${env.GPG_PASS}</gpg.passphrase>
      </properties>
    </profile>
  </profiles>
  
  <servers>
    <server>
      <id>ossrh</id>
      <username>$USER_NAME</username>
      <password>$USER_PASSWORD</password>
    </server>
  </servers>
</settings>
